<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="NSDevice">
	
	<!-- 实体 -->
	<resultMap type="Device"  id="device" >
		<id property="deviceId" column="deviceId" javaType="java.lang.String" />
		<result property="phone" column="phone" javaType="java.lang.String"/>
		<result property="phoneType" column="phoneType" javaType="java.lang.String" />
		<result property="phoneVersion" column="phoneVersion" javaType="java.lang.String" />
		<result property="appName" column="appName" javaType="java.lang.String" />
		<result property="appVersion" column="appVersion" javaType="java.lang.String" />
		<result property="userId" column="userId" javaType="java.lang.String" />
		<result property="userName" column="userName" javaType="java.lang.String" />
		<result property="createTime" column="createTime" javaType="java.util.Date" />
	</resultMap>

	<insert id="saveDevice" parameterType="Device">
		insert into t_device(deviceId, phone, phoneType, phoneVersion, appName, appVersion, userId, userName, createTime)
		values(#{deviceId}, #{phone}, #{phoneType}, #{phoneVersion}, #{appName}, #{appVersion}, #{userId}, #{userName}, #{createTime})
	</insert>



</mapper>
